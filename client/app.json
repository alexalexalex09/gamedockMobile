    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_SETTINGS",
        "WAKE_LOCK"
      ],
      "package": "com.alexscottbecker.gamedockMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bc8bbec9-5784-4e8f-9de3-ab13a3019a61"
      }
    }
  }
}
